<mvc:View
    controllerName="sap.ui.demo.walkthrough.controller.People"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<List
		headerText="{i18n>peopleListTitle}"
		class="sapUiResponsiveMargin"
		width="auto"
        items="{peoples>/value}">
		<items>
			<ObjectListItem
                title="{peoples>FirstName} {peoples>LastName}"
                />
		</items>
	</List>
</mvc:View>